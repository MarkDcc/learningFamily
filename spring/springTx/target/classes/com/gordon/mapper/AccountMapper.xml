<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gordon.mapper.AccountMapper">

    <insert id="addAccount" parameterType="com.gordon.pojo.Account">
        insert into account (id,name,num) values (#{id},#{name},#{num});
    </insert>

</mapper>